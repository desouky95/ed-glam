{
  "presets": [
    "@babel/preset-env",
    {
      "bugfixes": true,
      "modules": "auto"
    },
    "@babel/preset-react",
    "@babel/preset-typescript",
    "@babel/preset-flow"
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    "@babel/plugin-syntax-flow",
    "@babel/plugin-transform-template-literals"
  ]
}
